<Window x:Class="TextObservers.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:TextObservers"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <StackPanel>
        <TextBox x:Name="tbxTextBox" TextChanged="tbxTextBox_TextChanged"></TextBox>
        <Label x:Name="Label1"></Label>
        <Label x:Name="Label2"></Label>
        <Label x:Name="Label3"></Label>
        <Label x:Name="Label4"></Label>
        <Label x:Name="Label5"></Label>
    </StackPanel>
</Window>
